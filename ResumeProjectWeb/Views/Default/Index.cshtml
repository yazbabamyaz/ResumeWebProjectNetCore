@model IEnumerable<About>
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <title>Katmanlı Mimari Web Projesi</title>

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" type="text/css" href="/meet-me-opl/assets/css/bootstrap.min.css">
    <!-- Fonts -->
    <link rel="stylesheet" type="text/css" href="/meet-me-opl/assets/fonts/font-awesome.min.css">
    <!-- Icon -->
    <link rel="stylesheet" type="text/css" href="/meet-me-opl/assets/fonts/simple-line-icons.css">
    <!-- Slicknav -->
    <link rel="stylesheet" type="text/css" href="/meet-me-opl/assets/css/slicknav.css">
    <!-- Menu CSS -->
    <link rel="stylesheet" type="text/css" href="/meet-me-opl/assets/css/menu_sideslide.css">
    <!-- Slider CSS -->
    <link rel="stylesheet" type="text/css" href="/meet-me-opl/assets/css/slide-style.css">
    <!-- Nivo Lightbox -->
    <link rel="stylesheet" type="text/css" href="/meet-me-opl/assets/css/nivo-lightbox.css">
    <!-- Animate -->
    <link rel="stylesheet" type="text/css" href="/meet-me-opl/assets/css/animate.css">
    <!-- Main Style -->
    <link rel="stylesheet" type="text/css" href="/meet-me-opl/assets/css/main.css">
    <!-- Responsive Style -->
    <link rel="stylesheet" type="text/css" href="/meet-me-opl/assets/css/responsive.css">

</head>
<body>

    <!-- Header Area wrapper Starts -->
    <header id="header-wrap">
        <!-- Navbar Start -->
        <nav class="navbar navbar-expand-lg fixed-top scrolling-navbar indigo">
            <div class="container">
                <!-- Brand and toggle get grouped for better mobile display -->
                <div class="navbar-header">
                    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#main-navbar" aria-controls="main-navbar" aria-expanded="false" aria-label="Toggle navigation">
                        <span class="navbar-toggler-icon"></span>
                        <span class="icon-menu"></span>
                        <span class="icon-menu"></span>
                        <span class="icon-menu"></span>
                    </button>
                    <a href="index.html" class="navbar-brand"><img src="/meet-me-opl/assets/img/logo.png" alt=""></a>
                </div>
                <div class="collapse navbar-collapse" id="main-navbar">
                    <ul class="onepage-nev navbar-nav mr-auto w-100 justify-content-end clearfix">
                        <li class="nav-item active">
                            <a class="nav-link" href="#hero-area">
                                Home
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#about">
                                About
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#services">
                                Services
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#resume">
                                Resume
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#portfolios">
                                Work
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#contact">
                                Contact
                            </a>
                        </li>
                    </ul>
                </div>
            </div>

            <!-- Mobile Menu Start -->
            <ul class="onepage-nev mobile-menu">
                <li>
                    <a href="#home">Home</a>
                </li>
                <li>
                    <a href="#about">about</a>
                </li>
                <li>
                    <a href="#services">Services</a>
                </li>
                <li>
                    <a href="#resume">resume</a>
                </li>
                <li>
                    <a href="#portfolio">Work</a>
                </li>
                <li>
                    <a href="#contact">Contact</a>
                </li>
            </ul>
            <!-- Mobile Menu End -->
        </nav>
        <!-- Navbar End -->
        <!-- Hero Area Start -->
        <div id="hero-area" class="hero-area-bg">
            <div class="overlay"></div>
            <div class="container">
                <div class="row">
                    <div class="col-md-12 col-sm-12 text-center">
                        <div class="contents">

                            @foreach (var item in Model)
                            {
                                <h5 class="script-font wow fadeInUp" data-wow-delay="0.2s">@item.Slogan</h5>
                                <h2 class="head-title wow fadeInUp" data-wow-delay="0.4s">@item.Name</h2>
                                <p class="script-font wow fadeInUp" data-wow-delay="0.6s">@item.Profession</p>
                            }

                           
                          @await Component.InvokeAsync("Social2")
                            <div class="header-button wow fadeInUp" data-wow-delay="1s">
                                
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- Hero Area End -->

    </header>
    <!-- Header Area wrapper End -->
    <!-- About Section Start -->

    @await Component.InvokeAsync("About")

   
    <!-- About Section End -->
    <!-- Services Section Start -->
   @await Component.InvokeAsync("MyService")
    <!-- Services Section End -->
    <!-- Resume Section Start -->
    <div id="resume" class="section-padding">
        <div class="container">
            <div class="row">
               @await Component.InvokeAsync("Education")
               

               @await Component.InvokeAsync("Experience")
            </div>
        </div>
    </div>
    <!-- Resume Section End -->
    <!-- Portfolio Section -->
   @await Component.InvokeAsync("Work")
    <!-- Portfolio Section Ends -->
    <!-- Counter Area Start-->
   @await Component.InvokeAsync("Counter")
    <!-- Counter Area End-->
    <!-- Contact Section Start -->
    <section id="contact" class="section-padding">
        <div class="contact-form">
            <div class="container">
                <div class="row contact-form-area wow fadeInUp" data-wow-delay="0.4s">


                    @await Component.InvokeAsync("Contact")
                   
                   @await Component.InvokeAsync("ContactAddress")
                </div>
            </div>
        </div>
    </section>
    <!-- Contact Section End -->
    <!-- Footer Section Start -->
   @await Component.InvokeAsync("Social")
    <!-- Footer Section End -->
    <!-- Go to Top Link -->
    <a href="#" class="back-to-top">
        <i class="icon-arrow-up"></i>
    </a>

    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
    <script src="/meet-me-opl/assets/js/jquery-min.js"></script>
    <script src="/meet-me-opl/assets/js/popper.min.js"></script>
    <script src="/meet-me-opl/assets/js/bootstrap.min.js"></script>
    <script src="/meet-me-opl/assets/js/jquery.mixitup.js"></script>
    <script src="/meet-me-opl/assets/js/jquery.counterup.min.js"></script>
    <script src="/meet-me-opl/assets/js/waypoints.min.js"></script>
    <script src="/meet-me-opl/assets/js/wow.js"></script>
    <script src="/meet-me-opl/assets/js/jquery.nav.js"></script>
    <script src="/meet-me-opl/assets/js/jquery.easing.min.js"></script>
    <script src="/meet-me-opl/assets/js/nivo-lightbox.js"></script>
    <script src="/meet-me-opl/assets/js/jquery.slicknav.js"></script>
    <script src="/meet-me-opl/assets/js/main.js"></script>
    <script src="/meet-me-opl/assets/js/form-validator.min.js"></script>
    <script src="/meet-me-opl/assets/js/contact-form-script.min.js"></script>
    <script src="/meet-me-opl/assets/js/map.js"></script>

</body>
</html>